@{
    ViewData["Title"] = "Home Page";
}
@model PortofolioModel


@*Basculer sur des card de bootstrap*@


<div class="row">
    @foreach(KeyValuePair<string,string> Annee in Model.Annees)
    {
        <a href="PhotoAnnee?Annee=@Annee.Key"><br />
            <img class="imgPortofolio" src="@Annee.Value" />
            <div class="text-center">
                @Annee.Key
            </div>
        </a>
    }
</div>